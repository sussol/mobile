{
  "author": {
    "name": "Sustainable Solutions (NZ) Ltd.",
    "email": "info@sussol.net"
  },
  "name": "mSupplyMobile",
  "//": "version must be in the format ${majorNumber}.${minorNumber}.${patchNumber}-rc${releaseCandidateNumber}",
  "version": "3.0.4",
  "private": false,
  "license": "MIT",
  "description": "Mobile app for use with the mSupply medical inventory control software",
  "repository": {
    "type": "git",
    "url": "https://github.com/sussol/mobile.git"
  },
  "url": "http://github.com/sussol/mobile",
  "homepage": "http://msupply.org.nz",
  "scripts": {
    "start": "react-native run-android",
    "log": "react-native log-android",
    "shake": "adb shell input keyevent KEYCODE_MENU",
    "rr": "adb shell input text \"RR\"",
    "test": "jest --coverage",
    "build": "cd android && ./gradlew assembleRelease",
    "lint": "eslint src",
    "deploy": "./dev_scripts/deploy.sh",
    "export": "./dev_scripts/export.sh",
    "uninstall": "./dev_scripts/uninstall.sh",
    "clean-mac-cache": "rm -rf $TMPDIR/react-native-packager-* && rm -rf $TMPDIR/metro-bundler-* && watchman watch-del-all && rm -rf node_modules/ && yarn cache clean && yarn install"
  },
  "jest": {
    "preset": "react-native"
  },
  "resolutions": {
    "react-devtools-core": "3.6.0",
    "lodash": "4.17.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.5.0",
    "@react-navigation/core": "^3.0.3",
    "bugsnag-react-native": "^2.21.1",
    "prop-types": "^15.7.2",
    "react": "16.8.3",
    "react-native": "^0.59.10",
    "react-native-database": "^0.3.3",
    "react-native-device-info": "^2.1.3",
    "react-native-fs": "^2.13.3",
    "react-native-gesture-handler": "1.3.0",
    "react-native-localization": "2.1.2",
    "react-native-modalbox": "^1.7.1",
    "react-native-screens": "1.0.0-alpha.22",
    "react-native-ui-components": "^0.5.0",
    "react-native-vector-icons": "^6.6.0",
    "react-navigation": "^3.11.0",
    "react-navigation-redux-helpers": "^3.0.0",
    "react-navigation-tabs": "^0.4.0",
    "react-redux": "^6.0.0",
    "realm": "^2.29.0",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-error-handler": "^0.1.1",
    "redux-thunk": "^2.3.0",
    "set-manipulator": "^0.3.1",
    "sussol-utilities": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.1.0",
    "metro-react-native-babel-preset": "^0.51.1",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.7.0"
  }
}
